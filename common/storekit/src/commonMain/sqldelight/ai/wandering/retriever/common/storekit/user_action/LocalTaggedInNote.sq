CREATE TABLE localTaggedInNote(
    userId TEXT NOT NULL,
    noteId TEXT NOT NULL,
    FOREIGN KEY (userId) REFERENCES localUser (id),
    FOREIGN KEY (noteId) REFERENCES localNote (id),
    PRIMARY KEY (userId, noteId)
);

getById:
SELECT *
FROM localTaggedInNote
WHERE userId = ? AND noteId = ?;

upsert:
INSERT OR REPLACE INTO localTaggedInNote
VALUES ?;

clearById:
DELETE
FROM localTaggedInNote
WHERE userId = ? AND noteId = ?;

clearAll:
DELETE FROM localTaggedInNote;
